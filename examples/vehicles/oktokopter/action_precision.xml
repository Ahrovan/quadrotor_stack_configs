<?xml version="1.0"?>
<!-- variables used in droneActionMonitor, the variable is defined as a node-->
<!-- variables that define precision must begin with 'precision_' -->
<!-- the attribute 'value' correspond with the appropiate value of the node -->
<precision_take_off value="0.6"/>
<precision_land value="0.2"/>
<precision_null_axis_speed value="0.05"/>
<precision_null_rotation_speed value="0.12"/><!-- Used when checking wheter it has finished rotating -->
<precision_null_pose value="0.1"/>
<precision_null_attitude value="0.5"/><!-- degree, not used -->
<flip_front_pitch_value value="90"/><!-- degree, not used -->
<precision_null_rad_yaw value="0.25"/><!-- 0.25 radians equivalent to 11.4592 degrees -->
<!-- PID discretize by means of Bacward Euler Discretization Method s ~ (z-1)/(z*Ts) -->
<Speed_Controller>
        <Sampling_Time_Ts> 0.033 </Sampling_Time_Ts>  <!-- Sampling Time 30 Hz : 1/30 = 0.033 -->

	<PID_Vx2Pitch>
		<Gain>
			<Kp> 0.43 </Kp>  <!-- <Kp> 0.1   </Kp> Proportional Gain -->
			<Kd> -0.026329  </Kd>  <!-- <Kd> 0.06 </Kd> Derivative Gain   -->
			<Ki> 0.28531     </Ki>  <!-- <Ki> 0.0    </Ki> Integral Gain     -->
		</Gain>

		<Saturation> <!-- Control Input Saturation -->
			<UMax>  0.2 </UMax>  <!-- Max Saturation Value -->
			<UMin>  -0.2 </UMin>  <!-- Min Saturation Value -->
		</Saturation>

	        <FeedForward_Gain>
			<Gain>
				<!-- Laplace Domain: U_feedforward(s)/R(s) = (b-1)*Kp + ( (c-1)*Kd*N )/( 1 + N/s ) -->
				<b> 0.60894</b> <!-- Acts on the Proportional Gain -->
				<c> 1.9226 </c> <!-- Acts on the Derivative Gain -->
			</Gain>
		</FeedForward_Gain>

		<Derivative_Filter>
			<enable_derivative_filter> 1.0 </enable_derivative_filter> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Nd> 6.2751 </Nd>  <!-- Derivative filter gain N = 1/Tf -->
			</Gain>
		</Derivative_Filter>

		<Reference_Filter>
			<enable_reference_filter> 0.0 </enable_reference_filter> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Nr> 0.0 </Nr>  <!-- Reference filter gain N = 1/Tf -->
			</Gain>
		</Reference_Filter>

		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Kb> 10 </Kb> <!-- Anti Wind up gain : Laplace Domanin: U_anti_wind_up(s) = (Kb/s)*E_sat(s) -->
			</Gain>
		</Anti_wind_up>
	</PID_Vx2Pitch>

	<PID_Vy2Roll>
    <Gain>
			<Kp> 0.43 </Kp>  <!-- <Kp> 0.1   </Kp> Proportional Gain -->
			<Kd> -0.026329  </Kd>  <!-- <Kd> 0.06 </Kd> Derivative Gain   -->
			<Ki> 0.28531     </Ki>  <!-- <Ki> 0.0    </Ki> Integral Gain     -->
		</Gain>

		<Saturation> <!-- Control Input Saturation -->
			<UMax>  0.2 </UMax>  <!-- Max Saturation Value -->
			<UMin>  -0.2 </UMin>  <!-- Min Saturation Value -->
		</Saturation>

	        <FeedForward_Gain>
			<Gain>
				<!-- Laplace Domain: U_feedforward(s)/R(s) = (b-1)*Kp + ( (c-1)*Kd*N )/( 1 + N/s ) -->
				<b> 0.60894</b> <!-- Acts on the Proportional Gain -->
				<c> 1.9226 </c> <!-- Acts on the Derivative Gain -->
			</Gain>
		</FeedForward_Gain>

		<Derivative_Filter>
			<enable_derivative_filter> 1.0 </enable_derivative_filter> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Nd> 6.2751 </Nd>  <!-- Derivative filter gain N = 1/Tf -->
			</Gain>
		</Derivative_Filter>

		<Reference_Filter>
			<enable_reference_filter> 0.0 </enable_reference_filter> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Nr> 0.0 </Nr>  <!-- Reference filter gain N = 1/Tf -->
			</Gain>
		</Reference_Filter>

		<Anti_wind_up>
			<enable_anti_wind_up> 1.0 </enable_anti_wind_up> <!-- 1.0 means derivative filter enabled, 0.0 disabled -->
			<Gain>
				<Kb> 10 </Kb> <!-- Anti Wind up gain : Laplace Domanin: U_anti_wind_up(s) = (Kb/s)*E_sat(s) -->
			</Gain>
		</Anti_wind_up>
	</PID_Vy2Roll>

</Speed_Controller>
